{
  "name": "market-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "PORT=3009 next dev",
    "build": "PORT=3009 next build",
    "start": "PORT=3009 next start",
    "prod": "PORT=3009 next start",
    "lint": "eslint",
    "type-check": "tsc",
    "test": "NODE_ENV=test jest",
    "winTest": "SET NODE_ENV=test && jest",
    "winDev": "SET PORT=3009 && next dev",
    "winStart": "SET PORT=3009 && next start",
    "prepare": "husky install",
    "check-format": "prettier --check",
    "format": "prettier --write",
    "pretty-quick": "pretty-quick"
  },
  "dependencies": {
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "fingerprintjs2": "^2.1.4",
    "husky": "^9.0.10",
    "next": "14.1.0",
    "nextjs-toploader": "^1.6.6",
    "react": "^18",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18",
    "react-slick": "^0.30.1",
    "sass": "^1.70.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@types/fingerprintjs2": "^2.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "pretty-quick": "^4.0.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "src/**/*.(ts|tsx|js|jsx)": [
      "pretty-quick --staged",
      "npx eslint ./src",
      "next build ./"
    ]
  }
}
